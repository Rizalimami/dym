<?xml version="1.0" encoding="utf-8"?>
<!-- vim:fdn=3:-->
<openerp>
    <data>
        <template id="assets_backend" name="web_custom assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <!-- <link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css' /> -->
                <link href="/web/static/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
                <link rel="stylesheet" href="/web_custom_dayamotor/static/src/css/style.css" />
                <link rel="stylesheet" href="/web_custom_dayamotor/static/src/css/jquery.jscrollpane.css" />
                <script type="text/javascript" src="/web_custom_dayamotor/static/src/js/jquery.jscrollpane.min.js" />
            </xpath>
            <xpath expr="//script[@src='/web/static/src/js/view_form.js']" position="replace">
                <script type="text/javascript" src="/web_custom_dayamotor/static/src/js/view_form.js"></script>
            </xpath>
        </template>
        <template id="head" inherit_id="website.assets_frontend" name="snippets featured apps">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/web_custom_dayamotor/static/src/css/style_frontend.css" />
            </xpath>
        </template>
    </data>
</openerp>
