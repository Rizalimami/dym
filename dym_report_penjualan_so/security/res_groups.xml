<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

<!-- Laporan Penjualan -->
		
        <record id="group_dym_report_penjualan_so_wizard_read" model="res.groups">
            <field name="category_id" ref="dym_branch.tdm_role"/>
            <field name="name">Laporan Penjualan SO Read</field>
            <field name="menu_access" eval="[
            								(4, ref('menu_penjualan_so_report')),
            								]"/>
        </record>

        <record id="group_dym_report_penjualan_so_wizard_update" model="res.groups">
            <field name="category_id" ref="dym_branch.tdm_role"/>
            <field name="name">Laporan Penjualan SO Update</field>
            <field name="menu_access" eval="[
            								(4, ref('menu_penjualan_so_report')),
            								]"/>
			<field name="implied_ids" eval="[(4, ref('group_dym_report_penjualan_so_wizard_read'))]"/>
        </record>
        
        <record id="dym_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_dym_report_penjualan_so_wizard_read')),
											(4,ref('group_dym_report_penjualan_so_wizard_update')),
											]"/>
		</record>
    </data>
</openerp>
