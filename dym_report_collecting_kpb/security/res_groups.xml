<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">  	
    	
<!-- Laporan Collecting KPB -->	
        <record id="group_laporan_collecting_kpb_menu" model="res.groups">
            <field name="category_id" ref="dym_branch.tdm_role"/>
            <field name="name">Laporan Collecting KPB Menu</field> 
            <field name="menu_access" eval="[(4, ref('menu_collecting_kpb_report'))]"/>  
        </record>         
        <record id="group_laporan_collecting_kpb_mutasi_menu" model="res.groups">
            <field name="category_id" ref="dym_branch.tdm_role"/>
            <field name="name">Laporan Mutasi Oli KPB On Claim Menu</field> 
            <field name="menu_access" eval="[(4, ref('menu_collecting_kpb_mutasi_report'))]"/>  
        </record>         
   
        <record id="dym_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_laporan_collecting_kpb_menu')),(4,ref('group_laporan_collecting_kpb_mutasi_menu'))
                                            ]"/> 
		</record>              
        <record id="access_laporan_collecting_kpb_all" model="ir.model.access">
            <field name="model_id" ref="model_dym_report_collecting_kpb_wizard"/>
            <field name="name">dym_report_collecting_kpb_wizard all</field> 
            <field name="perm_read">1</field> 
            <field name="perm_create">1</field> 
            <field name="perm_write">1</field> 
            <field name="perm_unlink">1</field> 
        </record>                                                                              
    </data>
</openerp>
