<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">  	
    	
<!-- Laporan Piutang -->	
        <record id="group_laporan_piutang_invoice_finance_menu" model="res.groups">
            <field name="category_id" ref="dym_branch.tdm_role"/>
            <field name="name">Laporan Piutang Invoice Finance Menu</field> 
            <field name="menu_access" eval="[(4, ref('menu_piutang_invoice_report'))]"/>  
        </record> 

        <record id="group_laporan_piutang_invoice_read" model="res.groups">
            <field name="category_id" ref="dym_branch.tdm_role"/>
            <field name="name">Laporan Piutang Invoice Read</field> 
        </record> 

        <record id="group_laporan_piutang_invoice_update" model="res.groups">
            <field name="category_id" ref="dym_branch.tdm_role"/>
            <field name="name">Laporan Piutang Invoice Update</field> 
			<field name="implied_ids" eval="[(4, ref('group_laporan_piutang_invoice_read'))]"/>            	            	
        </record>
   
        <record id="dym_branch.group_sys_admin" model="res.groups">
			<field name="implied_ids" eval="[(4,ref('group_laporan_piutang_invoice_read')),
											(4,ref('group_laporan_piutang_invoice_update')),
                                            (4,ref('group_laporan_piutang_invoice_finance_menu')),
                                            ]"/> 
		</record>                                                                                           
    </data>
</openerp>
